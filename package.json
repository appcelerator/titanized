{
  "name": "titanized",
  "version": "0.2.2",
  "description": "Modernized APIs and utilities for Axway Titanium",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.prod.cjs.json",
    "build:esm": "tsc -p tsconfig.prod.esm.json",
    "clean": "rimraf dist",
    "lint": "tslint -p tsconfig.json && eslint test",
    "test": "npm run lint && npm run test:unit",
    "test:e2e": "wdio test/e2e/wdio.conf.js",
    "test:unit": "karma start test/unit/karma.unit.config.js",
    "prepack": "npm run build"
  },
  "keywords": [
    "titanium",
    "http",
    "axios",
    "dialogs",
    "promises"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "Axway Appcelerator",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appcelerator/titanized.git"
  },
  "bugs": {
    "url": "https://github.com/appcelerator/titanized/issues"
  },
  "homepage": "https://github.com/appcelerator/titanized#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/qs": "^6.5.1",
    "@types/titanium": "^7.3.0",
    "appium": "^1.9.1",
    "axios": "^0.18.0",
    "eslint": "^5.1.0",
    "eslint-config-axway": "^4.0.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-webdriverio": "^1.0.1",
    "fs-extra": "^7.0.0",
    "jasmine-core": "^3.1.0",
    "karma": "^3.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-titanium-launcher": "^0.3.1",
    "karma-typescript": "^4.0.0",
    "qs": "^6.5.2",
    "rimraf": "^2.6.2",
    "tempy": "^0.2.1",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-jasmine-rules": "^1.5.1",
    "typescript": "^3.1.3",
    "wdio-appium-service": "^0.2.3",
    "wdio-jasmine-framework": "^0.3.5",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.13.1"
  }
}
